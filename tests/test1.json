{
  "id": "standard test 1",

  "navigate": {
    "action": "navigate",
    "url": "http://127.0.0.1:8000"
  },
  "set first name": {
    "action"  : "type_text",
    "id"      : "edtFirstName",
    "value"   : "John"
  },
  "set last name": {
    "action"  : "type_text",
    "id"      : "edtLastName",
    "value"   : "Doe"
  },
  "set age": {
    "action"  : "type_text",
    "id"      : "edtAge",
    "value"   : 30
  },
  "check first name value": {
    "action"  : "assert_value_eq",
    "id"      : "edtFirstName",
    "value"   : "John"
  },
  "check last name value": {
    "action"  : "assert_value_eq",
    "id"      : "edtLastName",
    "value"   : "Doe"
  },
  "check age value": {
    "action"  : "assert_value_eq",
    "id"      : "edtAge",
    "value"   : 33
  },
  "check age is not zero": {
    "action"  : "assert_value_neq",
    "id"      : "edtAge",
    "value"   : 0
  }
}


